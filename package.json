{
  "name": "s3driver",
  "version": "1.0.52",
  "description": "Simple driver for AWS S3 compatible storage",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/esbuild ./src/index.js --format=esm --outfile=./dist/index.js --platform=node --target=node16.3 --minify",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheGP/s3driver.git"
  },
  "keywords": [
    "driver",
    "s3",
    "filesystem"
  ],
  "author": "Eugene Bos",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1545.0",
    "debug": "^4.3.4",
    "file-type": "^19.0.0",
    "mime-types": "^2.1.35",
    "p-queue": "^8.0.1",
    "promise-queue": "^2.2.5"
  },
  "devDependencies": {
    "esbuild": "^0.20.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-serial-runner": "^1.2.1"
  }
}
